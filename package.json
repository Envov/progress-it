{
  "name": "progress-boolean",
  "version": "1.0.0",
  "description": "map boolean as floating number",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "lib/",
    "esm/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UILKUNP/progress-boolean.git"
  },
  "author": "UILUNP",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UILKUNP/progress-boolean/issues"
  },
  "homepage": "https://github.com/UILKUNP/progress-boolean#readme",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "next",
        "prerelease": "rc"
      }
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "lint-staged": {
    "src/**/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "volta": {
    "node": "14.17.6",
    "yarn": "1.22.11"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-use"
  }
}
